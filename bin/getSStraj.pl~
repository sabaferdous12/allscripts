use strict;
use warnings;

use general qw (readDirPDB);
my $dir = ".";

my @PDBfiles = readDirPDB($dir);

foreach my $pdb ( @PDBfiles) {
    chomp $pdb;

    print "$pdb\n";
    
    `pdbsecstr -s $pdb | awk '{""; print \$3}' >"$pdb.txt"`;
   # last;
#    `paste -d "" "$pdb.txt" >>ss. txt`;

    
}

